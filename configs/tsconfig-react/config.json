{
  "packageInfo": {
    "devDependencies": {
      "typescript": "^4.3.2"
    }
  },
  "input": {
    "checkFiles": ["tsconfig.json"],
    "config": {
      "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "allowJs": true,
        "strict": true,
        "target": "ESNext",
        "outDir": "./dist",
        "sourceMap": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "jsx": "react-jsx",
        "lib": ["dom", "esnext"],
        "baseUrl": "./",
        "paths": {
          "@/*": ["src/*"]
        }
      },
      "typeRoots": ["node_modules/@types"],
      "exclude": ["node_modules", "dist", "build"]
    }
  },
  "output": {
    "files": ["tsconfig.json"]
  }
}
